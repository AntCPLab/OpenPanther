{
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.autocomplete": "default",
    "clangd.arguments": [
        "--header-insertion=never"
    ],
    "testMate.cpp.discovery.testListCaching": true,
    "testMate.cpp.test.parallelExecutionOfExecutableLimit": 8,
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "bazel-bin/**/*{_test}",
            "cwd": "${workspaceFolder}",
            "gtest": {
                "testGrouping": {
                    "groupBySource": {
                        "label": "${sourceRelPath[1:]}"
                    }
                }
            }
        }
    ],
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/.cache/**": true,
        "**/bazel-*/**": true,
        "**/external/**": true
    },
    "bazel.enableCodeLens": true,
    "editor.formatOnSave": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "[dockerfile]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.formatOnSave": false,
    },
    "python.formatting.provider": "none",
    "esbonio.sphinx.confDir": "",
    "git.ignoreLimitWarning": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "mlir.server_path": "bazel-bin/libspu/compiler/tools/pphlo-lsp",
    // "bsv.bazel.buildFlags": [
        // "--config=custom",
    // ],
    "bsv.cc.compdb.targets": [
        // "//experimental/squirrel:utils_test",
        "//experimental/ann:dist_cmp",
        "//experimental/ann:batch_argmax_test",
        // "//app/bar:bar_binary",
        // "//app/baz:baz_binary",
    ],
    "C_Cpp.intelliSenseEngineFallback": "enabled",
    "files.associations": {
